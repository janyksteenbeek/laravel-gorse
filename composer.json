{
    "name": "janyksteenbeek/laravel-gorse",
    "homepage": "https://github.com/janyksteenbeek/laravel-gorse",
    "description": "Laravel integration for Gorse recommendation engine",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JanykSteenbeek\\LaravelGorse\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Janyk Steenbeek",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JanykSteenbeek\\LaravelGorse\\GorseServiceProvider"
            ],
            "aliases": {
                "Gorse": "JanykSteenbeek\\LaravelGorse\\Facades\\Gorse"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^10.1",
        "laravel/pint": "^1.21",
        "phpstan/phpstan": "^2.1"
    }
}
